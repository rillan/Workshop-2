      <TextField fx:id="boatID" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="194.0" promptText="Enter ID" />
      <TextField fx:id="updateBoatType" layoutX="14.0" layoutY="47.0" prefHeight="25.0" prefWidth="194.0" promptText="Enter a new type" />
      <TextField fx:id="updateBoatLength" layoutX="14.0" layoutY="83.0" prefHeight="25.0" prefWidth="194.0" promptText="Enter a new length" />
      <TextField fx:id="error" layoutX="14.0" layoutY="140.0" text="Something is wrong" visible="false" />
   </children>
</AnchorPane>
